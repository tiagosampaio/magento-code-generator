{
    "name": "tiagosampaio/magento-code-generator",
    "description": "Code Generator for PHP.",
    "type": "project",
    "keywords": ["code-generator", "php"],
    "license": "OSL-3",
    "authors": [
        {
            "name": "Tiago Sampaio",
            "email": "tiago@tiagosampaio.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">7.1",
        "laminas/laminas-code": "^3.4",
        "symfony/filesystem": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MagentoCodeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MagentoCodeGeneratorTest\\": "dev/test/unit/"
        }
    }
}
